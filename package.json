{
  "name": "auth0-lock",
  "version": "11.32.2",
  "description": "Auth0 Lock",
  "author": "Auth0 <support@auth0.com> (http://auth0.com)",
  "license": "MIT",
  "keywords": [
    "auth0",
    "auth",
    "openid",
    "authentication",
    "passwordless",
    "browser",
    "jwt"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/auth0/lock"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "grunt dev",
    "build": "grunt build",
    "design": "grunt design",
    "dev": "grunt dev",
    "dist": "grunt dist",
    "prepublish": "cross-env BABEL_ENV=npm grunt dist",
    "precommit": "yarn lint-staged",
    "lint": "eslint --ext .jsx,.js src/",
    "test:e2e": "cross-env BABEL_ENV=test karma start --browsers ChromeHeadless",
    "test:e2e:watch": "cross-env BABEL_ENV=test karma start --browsers Chrome --single-run=false",
    "test:e2e:browserstack": "cross-env BABEL_ENV=test karma start",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch --coverage",
    "test:es-check": "es-check es5 'build/*.js'",
    "publish:cdn": "ccu --trace",
    "i18n:translate": "grunt dist && node scripts/complete-translations.js && npm run i18n:prettier && npm run build",
    "i18n:prettier": "prettier --write src/i18n/*",
    "i18n:validate": "node -r esm scripts/lang-audit.js"
  },
  "devDependencies": {
    "@auth0/component-cdn-uploader": "^2.2.2",
    "@google-cloud/translate": "^6.0.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-stylus-compiler": "^1.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-css-import-to-string": "0.0.2",
    "babel-plugin-version-inline": "^1.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^8.0.0",
    "browserify": "^17.0.0",
    "bump-version": "^0.5.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^0.26.1",
    "emojic": "^1.1.15",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "es-check": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.4.0",
    "esm": "^3.2.25",
    "expect.js": "^0.3.1",
    "flat": "^5.0.2",
    "glob": "^7.1.6",
    "grunt": "^1.3.0",
    "grunt-babel": "^6.0.0",
    "grunt-cli": "^0.1.13",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-env": "^0.4.4",
    "grunt-exec": "^0.4.6",
    "grunt-webpack": "^2.0.1",
    "husky": "^7.0.2",
    "jest": "^21.2.1",
    "json-beautify": "^1.0.1",
    "karma": "^6.3.4",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-browserify": "^8.1.0",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.1",
    "node-es-module-loader": "^0.3.8",
    "prettier": "^2.4.1",
    "puppeteer": "^10.1.0",
    "react-test-renderer": "^15.6.2",
    "sinon": "^1.15.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "tmp": "^0.1.0",
    "uglify-js": "^2.7.4",
    "unminified-webpack-plugin": "^1.1.1",
    "unreleased": "^0.1.0",
    "watchify": "^4.0.0",
    "webpack": "^2.2.1",
    "webpack-core": "^0.6.8",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "auth0-js": "^9.19.0",
    "auth0-password-policies": "^1.0.2",
    "blueimp-md5": "^2.19.0",
    "classnames": "^2.3.1",
    "dompurify": "^2.3.4",
    "immutable": "^3.7.3",
    "jsonp": "^0.2.1",
    "node-fetch": "^2.6.7",
    "password-sheriff": "^1.1.1",
    "prop-types": "^15.8.0",
    "qs": "^6.10.3",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-transition-group": "^2.2.1",
    "trim": "^1.0.1",
    "url-join": "^1.1.0",
    "validator": "^13.6.0"
  },
  "resolutions": {
    "node-fetch": "^2.6.1",
    "crypto-js": "^4.1.1"
  },
  "ccu": {
    "name": "lock",
    "cdn": "https://cdn.auth0.com",
    "mainBundleFile": "lock.min.js",
    "bucket": "assets.us.auth0.com",
    "localPath": "build",
    "digest": {
      "hashes": [
        "sha384"
      ],
      "extensions": [
        ".js"
      ]
    }
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src/",
      "<rootDir>/src/__tests__"
    ],
    "setupFiles": [
      "<rootDir>/src/__tests__/setup-tests.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/",
      "<rootDir>/lib/",
      "<rootDir>/src/__tests__/testUtils.js",
      "<rootDir>/src/__tests__/setup-tests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test/",
      "<rootDir>/lib/",
      "<rootDir>/src/__tests__/testUtils.js",
      "<rootDir>/src/__tests__/setup-tests.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint"
    ],
    "*.{js,jsx,json}": [
      "prettier --write"
    ]
  }
}
