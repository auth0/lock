{
  "extends": ["plugin:react/recommended", "prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/display-name": 0,
    "react/prop-types": 1,
    "react/no-find-dom-node": 1,
    "react/no-string-refs": 1,
    "react/no-danger": 2
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": ["build/"]
}
